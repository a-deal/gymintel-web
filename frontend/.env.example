# GymIntel Web Frontend Environment Variables

# GraphQL API Endpoints
VITE_GRAPHQL_ENDPOINT=http://localhost:8000/graphql
VITE_GRAPHQL_WS_ENDPOINT=ws://localhost:8000/graphql

# Mapbox Configuration (required for interactive maps)
VITE_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token-here

# App Configuration
VITE_APP_TITLE=GymIntel Web
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SUBSCRIPTIONS=true